<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>  
    <listener-class>
        servlets.StartupListener
    </listener-class>
  </listener>  	

  <servlet>
  	<servlet-name>RESTInterface</servlet-name>
  	<display-name>RESTInterface</display-name>
  	<description></description>
  	<servlet-class>servlets.RESTInterface</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RESTInterfaceAlternative</servlet-name>
  	<display-name>RESTInterfaceAlternative</display-name>
  	<description></description>
  	<servlet-class>servlets.RESTInterfaceAlternative</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>RESTInterface</servlet-name>
  	<url-pattern>/RESTInterface</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RESTInterfaceAlternative</servlet-name>
  	<url-pattern>/RESTInterfaceAlternative</url-pattern>
  </servlet-mapping>
</web-app>
